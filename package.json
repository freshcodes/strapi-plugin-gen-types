{
  "name": "@fresh.codes/strapi-plugin-gen-types",
  "description": "Auto-generate TypeScript types after Strapi startup",
  "license": "ISC",
  "author": "Fresh Codes <hello@fresh.codes>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freshcodes/strapi-plugin-gen-types.git"
  },
  "bugs": {
    "url": "https://github.com/freshcodes/strapi-plugin-gen-types/issues"
  },
  "homepage": "https://github.com/freshcodes/strapi-plugin-gen-types#readme",
  "version": "1.0.0",
  "keywords": [
    "strapi",
    "plugin",
    "typescript",
    "codegen"
  ],
  "strapi": {
    "kind": "plugin",
    "name": "gen-types",
    "displayName": "Generate Types",
    "description": "Auto-generate TypeScript types after Strapi startup"
  },
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write --ignore-unknown .",
    "test": "tsx --test server/src/**/*.test.ts",
    "test:watch": "tsx --test --watch server/src/**/*.test.ts",
    "prepare": "husky",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.23.1",
    "@strapi/typescript-utils": "^5.23.1",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.18.1"
  }
}
